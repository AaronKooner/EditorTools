			<key>match</key>
			<string>\b(?i:(abs|ceiling|floor|ln|log10|mod|min|max|round|sqrt|sin|cos|tan|arcsin|arccos|arctan|arctan2|constant))\b</string>
			<key>name</key>
			<string>support.function.kos</string>
