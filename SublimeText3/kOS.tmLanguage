		<dict>
			<key>comment</key>
			<string>assignment operators and directives</string>
			<key>match</key>
			<string>\b(?i:(declare|parameter|local|global|set|to|is|\=|@lazyglobal))\b</string>
			<key>name</key>
			<string>keyword.operator.assignment.kos</string>
		</dict>
		<dict>
			<key>comment</key>
			<string>comparison operator</string>
			<key>match</key>
